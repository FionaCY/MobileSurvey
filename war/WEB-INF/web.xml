		<url-pattern>/questionedit</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>QuestionDelete</servlet-name>
		<servlet-class>com.gae.mobilesurvey.QuestionDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QuestionDelete</servlet-name>
		<url-pattern>/questiondelete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Questionnaire</servlet-name>
		<servlet-class>com.gae.mobilesurvey.QuestionnaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Questionnaire</servlet-name>
		<url-pattern>/questionnaire</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>QuestionList</servlet-name>
		<servlet-class>com.gae.mobilesurvey.QuestionListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QuestionList</servlet-name>
		<url-pattern>/questionlist</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>surveylist.jsp</welcome-file>
	</welcome-file-list>
</web-app>
